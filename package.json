{
  "name": "adaptive-data-fusion-website",
  "version": "1.0.0",
  "description": "Adaptive Data Fusion Custom Website with Node Homepage Design",
  "keywords": [
    "react",
    "redux",
    "data-fusion",
    "web-application"
  ],
  "homepage": "https://github.com/derrickbass2/ADF-Website#readme",
  "bugs": {
    "url": "https://github.com/derrickbass2/ADF-Website/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/derrickbass2/ADF-Website.git"
  },
  "license": "Apache-2.0",
  "author": "Derrick Bass",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "main": "src/main.tsx",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings 0",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc && vite build",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss}'"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.7",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.14",
    "@reduxjs/toolkit": "^2.2.5",
    "axios": "^1.7.2",
    "bootstrap": "^5.3.3",
    "d3": "^7.9.0",
    "framer-motion": "^11.2.10",
    "glob": "^9.3.5",
    "gsap": "^3.12.5",
    "openai": "^4.33.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.23.1",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/eslintrc": "^2.0.0",
    "@eslint/js": "^9.23.0",
    "@types/d3": "^7.4.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-legacy": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "globals": "^13.20.0",
    "jest": "^29.7.0",
    "lru-cache": "^11.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.77.2",
    "typescript": "^5.8.2",
    "vite": "^6.0.0",
    "vite-plugin-checker": "^0.9.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "rimraf": "^6.0.1",
    "glob": "^9.3.5"
  }
}
